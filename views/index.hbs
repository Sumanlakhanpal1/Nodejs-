     {{!-- //contact us section --}}
     <section class="py-5 background1" id="contact-section" >
      <div class="container" >
        <h3 class="text-center  cs1"  > Contact us</h3>
        <div class="row">
          <div class="col-md-12">
                <form method="post" action="/process-contact-form">
  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label cs1">Email address</label>
    <input name="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    <div id="emailHelp" class="form-text cs1">We'll never share your email with anyone else.</div>
  </div>
  <div class="mb-3">
    <label for="exampleInputPassword1" class="form-label cs1">Contact number</label>
    <input name="phone" class="form-control" id="exampleInputPassword1">
  </div>
  
  <div class="form-floating my-4">
  <textarea  name ="query" style="height: 200px"; class="form-control" placeholder="Enter a query" id="floatingTextarea"></textarea>
  <label for="floatingTextarea">Your query</label>
</div>
  <div class="container text-center">
    <button type="submit" class="btn btn-primary" >Submit</button>
  </div>
  
</form>
