 <section id="services-section">
  <div class="container">
      <h4 class="text-center text-secondary py-3">Services that we provide</h4>
         
      <div class="row  ">
        {{#each services}}
        <div class="col-md-4 py-3">
          <div class="card ">
            <div class="card-body">
        <div class="card-title">
          <div class="container"  >
             <i class="{{this.icon}}"></i>
          </div>
         
          <h4>{{this.title}}</h4>
          <p>{{this.description}}</p>
          <a href="{{this.linktext}}">check</a>
          <a href="{{this.link}}">Support</a>
        </div>
        </div>
        </div>
      </div>
          {{/each}}
      </div>
   
     </div>
 </section>
     
